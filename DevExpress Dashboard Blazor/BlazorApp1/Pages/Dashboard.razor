@page "/dashboard"
@inject IJSRuntime JSRuntime
@implements IDisposable
@*<div id="web-dashboard" style="width:100%; height: 800px;">*@
<div id="web-dashboard" style="position:relative;">
</div>

@code {
    protected override void OnAfterRender(bool firstRender)
    {
        JSRuntime.InvokeAsync<object>("JsFunctions.InitWebDashboard");
    }

    public void Dispose()
    {
        JSRuntime.InvokeAsync<string>("JsFunctions.DisposeWebDashboard");
    }


}